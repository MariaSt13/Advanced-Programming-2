
@{
    ViewBag.Title = "DeletePhotoView";
}
<!DOCTYPE html>
<html>
<body>
    
    <p>Are you sure you want to delete this photo?</p>
    <img src=@Model.photo.Path>
    <p>Photo name: @Model.photo.Name </p>
    <p>@Model.photo.Year </p>
    <p>@Model.photo.Month</p>
    <p><a href="#" class="btn btn-info btn-md" onclick="location.href='@Url.Action("PhotosView", "First")'" role="button">No</a> <a href="#" class="btn btn-danger btn-md" onclick="location.href='@Url.Action("DeletePhoto", "First")'" role="button">Yes</a></p>

</body>
</html>
